    "eslint": "^9.25.1",
    "prettier": "^3.5.3",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.31.1"
  },
  "dependencies": {
    "@atproto/api": "^0.13.35",
    "@atproto/bsky": "^0.0.101",
